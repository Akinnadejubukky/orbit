// @flow

import * as React from "react";
import { ReactComponentStyled } from "styled-components";

type ImageObject = {|
  "*"?: string,
  jpg?: string,
  webp?: string,
|};

export type Props = {|
  placeholder?: ImageObject,
  original: ImageObject,
  name: string,
|};

export type PictureProps = {|
  pictures: ImageObject,
  name: string,
  loaded: boolean,
  onLoad?: () => void,
  lowRes?: boolean,
|};

export type LazyImage = React.ComponentType<Props>;

declare export default LazyImage;

declare export var StyledLazyImage: ReactComponentStyled<any>;
