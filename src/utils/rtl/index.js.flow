// @flow
import type { ThemeProps } from "../../defaultTheme";

type Props = { ...ThemeProps };

export type LeftToRight = <T1, T2>(left: T1, right: T2) => (props: Props) => T1 | T2;

export type RtlSpacing = (value: string) => (props: Props) => string;

export type BorderRadius = (value: string) => (props: Props) => string;

export type TextAlign = (value: "left" | "right") => (props: ThemeProps) => string;

export type Translate3d = (value: string) => (props: Props) => string;

declare export var rtlSpacing: RtlSpacing;

declare export var left;

declare export var right;

declare export var borderRadius: BorderRadius;

declare export var textAlign: TextAlign;

declare export var translate3d: Translate3d;
